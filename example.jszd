entity Book
    isbn: string
    [OneToMany, Fetch=EAGER, Cascade=ALL]
    author: Author
    title: string
    plural = 'Books'

entity Author
    [Length=13, Unique]
    jmbg: string
    [Length=256]
    name: string
    [NotNullable]
    lastname: string
    [ManyToOne]
    books: Array<Book>
    book: Book